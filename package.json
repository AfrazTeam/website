{
	"name": "afrazteam",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"prepare": "husky install"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=18.17.0",
		"yarn": "please-use-pnpm",
		"npm": "please-use-pnpm"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write .",
			"eslint --fix ."
		]
	},
	"dependencies": {
		"animate.css": "^4.1.1",
		"next": "13.4.7",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@eslint/create-config": "^0.4.6",
		"@types/node": "20.3.3",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"autoprefixer": "10.4.14",
		"eslint": "8.44.0",
		"eslint-config-next": "13.4.7",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"husky": "^8.0.0",
		"lint-staged": "^15.2.2",
		"postcss": "8.4.24",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"tailwindcss": "3.3.2",
		"typescript": "5.1.6"
	}
}
